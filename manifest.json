{
    "manifest_version": 2,
    "name": "Startpage",
    "version": "1.2.0",
    "description": "Custom new tab startpage",
    "chrome_url_overrides": {
      "newtab": "startpage.html"
    },
    
    "content_scripts": [
	  {
		"matches":[ "<all_urls>" ],
		"js": [ "/script.js" ]
	  }
    ],
    
    "content_security_policy": "script-src 'self'; script-src 'unsafe-inline'; object-src 'self';"
}
